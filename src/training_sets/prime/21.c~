#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
#include <math.h>
 
void main()
{
 int M, N, i, j, flag, temp, count = 0;
 
 clrscr();
 
 printf("Enter the value of N\n");
 scanf("%d",&N);

  for (j=2; j<=i/2; j++)
  {
   if( (i%j) == 0)
   {
    flag = 1;
    break;
   }
  }
  if(flag == 0)
  {
   printf("%d\n",i);
  }
 }
 printf("Number of primes between %d  and %d = %d\n",temp,N,count);
}
